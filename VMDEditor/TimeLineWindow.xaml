<Window x:Name="WindowTimeLine" x:Class="VMDEditor.TimeLineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VMDEditor"
        mc:Ignorable="d"
        Title="TimeLine" Height="282" Width="800" Closing="WindowTimeLine_Closing">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition x:Name="FrameRow" Height="13*"/>
            <RowDefinition Height="120*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition x:Name="ArticleColumn"/>
            <ColumnDefinition Width="9*"/>
        </Grid.ColumnDefinitions>
        <ScrollViewer x:Name="FrameScroll" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled" Grid.Column="1" Grid.Row="0">
            
        </ScrollViewer>
        <ScrollViewer x:Name="ArticleScroll" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Hidden" Grid.Column="0" Grid.Row="1">

        </ScrollViewer>
        <ScrollViewer x:Name="TimeLineScroll" HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible" Grid.Column="1" Grid.Row="1">

        </ScrollViewer>
    </Grid>
</Window>
